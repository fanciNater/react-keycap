{
		"name": "react-keycap",
		"version": "0.0.4",
		"description": "React rapid development component library.",
		"readme": "https://github.com/ispong/react-keycap/blob/master/README.md",
		"private": false,
		"keywords": [
				"react",
				"keycap",
				"react-component"
		],
		"homepage": "http://ispong.gitee.io",
		"bugs": {
				"url": "https://github.com/ispong/react-keycap/issues",
				"email": "ispong@lookout.com"
		},
		"license": "MIT",
		"author": {
				"name": "ispong",
				"email": "ispong@outlook.com",
				"url": "http://ispong.gitee.io"
		},
		"repository": {
				"type": "git",
				"url": "https://github.com/ispong/react-keycap"
		},
		"main": "lib/index.js",
		"module": "es/index.js",
		"typings": "typings/index.d.ts",
		"files": [
				"es",
        "typings"
		],
		"scripts": {
				"link": "npm link",
        "build:es": "babel ./components -d ./es/ --extensions \".tsx\" ",
        "cpx:es": "cpx \"./components/**/*.sass\" ./es",
        "build:commonjs": "cross-env BABEL_OUTPUT=commonjs babel ./components -d ./lib/ --plugins @babel/plugin-transform-modules-commonjs --extensions \".tsx\" ",
        "build:dts-gen": "dts-gen -m react-keycap -f ./es/index.d.ts -o",
        "cpx:commonjs": "cpx \"./components/**/*.sass\" ./lib",
        "prettier": "prettier -c --write '**/*'"
		},
		"dependencies": {
				"@babel/runtime": "^7.10.4",
				"@testing-library/jest-dom": "^4.2.4",
				"@testing-library/react": "^9.5.0",
				"@testing-library/user-event": "^7.2.1",
				"react": "^16.13.0",
				"react-dom": "^16.13.0",
				"react-scripts": "3.4.0"
		},
		"devDependencies": {
				"@babel/cli": "^7.2.3",
				"@babel/core": "^7.11.0",
				"@babel/plugin-transform-modules-commonjs": "^7.10.4",
				"@babel/preset-env": "^7.10.4",
				"@babel/preset-react": "^7.8.3",
				"@babel/preset-typescript": "^7.10.4",
				"@types/node": "^14.0.27",
				"babel-jest": "^26.1.0",
				"babel-loader": "^8.1.0",
				"clean-webpack-plugin": "^3.0.0",
				"copy-webpack-plugin": "^6.0.3",
				"cpx": "^1.5.0",
				"cross-env": "^7.0.2",
				"css-loader": "^3.6.0",
				"jest": "^26.1.0",
				"node-sass": "^4.14.1",
				"prettier": "^2.0.2",
				"sass-loader": "^9.0.2",
				"style-loader": "^1.2.1",
				"ts-loader": "^8.0.1",
				"typescript": "^3.9.7",
				"webpack": "^4.43.0",
				"webpack-cli": "^3.3.12"
		}
}
