{
		"name": "react-keycap",
		"version": "0.0.4",
		"description": "React rapid development component library.",
		"readme": "https://github.com/ispong/react-keycap/blob/master/README.md",
		"private": false,
		"keywords": [
				"react",
				"keycap",
				"react-component"
		],
		"homepage": "http://ispong.gitee.io",
		"bugs": {
				"url": "https://github.com/ispong/react-keycap/issues",
				"email": "ispong@lookout.com"
		},
		"license": "MIT",
		"author": {
				"name": "ispong",
				"email": "ispong@outlook.com",
				"url": "http://ispong.gitee.io"
		},
		"repository": {
				"type": "git",
				"url": "https://github.com/ispong/react-keycap"
		},
		"main": "lib/index.js",
		"module": "es/index.js",
		"typings": "lib/index.d.ts",
		"files": [
				"es",
				"lib"
		],
		"scripts": {
				"link": "npm link",
				"build:rollup": "rollup -c ./rollup.config.js",
				"clean:es": "del /s/q es",
				"build:es": "babel ./components -d ./es/ --extensions \".tsx\" ",
				"cpx:es": "cpx \"./components/**/*.sass\" ./es",
				"build:commonjs": "cross-env BABEL_OUTPUT=commonjs babel ./components -d ./lib/ --plugins @babel/plugin-transform-modules-commonjs --extensions \".tsx\" ",
				"build:dts-gen": "dts-gen -m react-keycap -f ./es/index.d.ts -o",
				"cpx:commonjs": "cpx \"./components/**/*.sass\" ./lib",
				"prettier": "prettier -c --write '**/*'"
		},
		"dependencies": {
				"@babel/runtime": "^7.10.4",
				"@testing-library/jest-dom": "^4.2.4",
				"@testing-library/react": "^9.5.0",
				"@testing-library/user-event": "^7.2.1",
				"react": "^16.13.0",
				"react-dom": "^16.13.0",
				"react-scripts": "3.4.0"
		},
		"devDependencies": {
				"@babel/cli": "^7.2.3",
				"@babel/core": "^7.11.0",
				"@babel/plugin-transform-modules-commonjs": "^7.10.4",
				"@babel/preset-env": "^7.10.4",
				"@babel/preset-react": "^7.8.3",
				"@babel/preset-typescript": "^7.10.4",
				"@rollup/plugin-babel": "^5.1.0",
				"@rollup/plugin-node-resolve": "^8.4.0",
				"@rollup/plugin-sucrase": "^3.1.0",
				"@rollup/plugin-typescript": "^5.0.2",
				"cpx": "^1.5.0",
				"cross-env": "^7.0.2",
				"jest": "^26.1.0",
				"node-sass": "^4.14.1",
				"prettier": "^2.0.2",
				"typescript": "^3.9.7"
		}
}
